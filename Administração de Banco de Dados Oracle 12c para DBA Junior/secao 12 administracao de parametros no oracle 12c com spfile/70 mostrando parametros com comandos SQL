 SELECT name, ispdb_modifiable FROM v$parameter
  2  WHERE name = 'db_big_table_cache_percent_target';




SELECT NAME,VALUE,ISSES_MODIFIABLE,ISSYS_MODIFIABLE
FROM V$PARAMETER 
WHERE NAME LIKE '%sga%';



COL NAME FOR A32



